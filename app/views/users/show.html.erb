<% provide(:title, @user.name) %>
<div class="row">
	<aside class="col-mid-4">
		<section class="user_info">
			<h1>
				<%= gravatar_for @user %>
				<%= @user.name %>
			</h1>
		</section>
	</aside>
</div>



<p>
Joined:   <%= @user.created_at %><br> 
Lastseen: <%= @user.updated_at %><br>
Time:     <%= Time.now %>
</p>